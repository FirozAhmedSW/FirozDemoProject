@model WebApplication1.Models.User
@{
    ViewData["Title"] = "Edit Details";
}

<div class="card mb-3" style="max-width: 600px;">
    <div class="card-body">
        <h5 class="card-title">@Model.UserName</h5>
        <p class="card-text"><strong>Email:</strong> @Model.Email</p>
        <p class="card-text"><strong>Address:</strong> @Model.Address</p>
        <p class="card-text"><strong>Contact:</strong> @Model.Contact</p>
        <p class="card-text"><strong>About:</strong> @Model.About</p>
    </div>
</div>

<a asp-action="Index" class="btn btn-secondary">Back</a>
